  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && npx tsc",
    "start": "npm run build && node dist/index.js",
    "dev": "cross-env NODE_ENV=development npm run build && concurrently \"cross-env NODE_ENV=development npx tsc -w\" \"cross-env NODE_ENV=development nodemon --exec ts-node src/server.ts\"",
    "mini:prod": "cross-env NODE_ENV=production npm run build && concurrently \"cross-env NODE_ENV=production npx tsc -w\" \"cross-env NODE_ENV=production nodemon --exec ts-node src/server.ts\""
  },
